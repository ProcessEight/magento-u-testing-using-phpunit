<config>
    <modules>
        <Training_Example>
            <version>0.1.0</version>
        </Training_Example>
    </modules>
    <global>
        <models>
            <training_example>
                <class>Training_Example_Model</class>
                <resourceModel>training_example_resource</resourceModel>
            </training_example>
            <training_example_resource>
                <class>Training_Example_Model_Resource</class>
            </training_example_resource>
        </models>
        <blocks>
            <training_example>
                <class>Training_Example_Block</class>
            </training_example>
        </blocks>
        <helpers>
            <training_example>
                <class>Training_Example_Helper</class>
            </training_example>
        </helpers>
    </global>
    <frontend>
        <layout>
            <updates>
                <training_example>
                    <file>training/example.xml</file>
                </training_example>
            </updates>
        </layout>
    </frontend>
</config>